package or.kr.connect;

import org.junit.Test;
import org.junit.Before;

public class CountTest {
    private static int count = 0;
    @Before //test before
    public void setUp() {
        System.out.print(count++);
    }

    @Test //test method
    public void testPlus() {
        System.out.print(count++);
    }

    @Test //test method
    public void increase (){
        System.out.print(count++);
    }
}